                            </div>
                            <div class="col-sm-9 text-secondary">
                                @Model.DigitalWalletId
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Referral</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                @Model.IdentifireEmail
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Digital Pin</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                @Model.IdentifireKey
                            </div>
                        </div>
                        <hr/>
                        <div class="row">
                            <div class="col-sm-3">
                                <h6 class="mb-0">Selected Plan</h6>
                            </div>
                            <div class="col-sm-9 text-secondary">
                                @Model.SelectedPlan
                            </div>
                        </div>
